package classes {     import flash.display.Loader;  	import flash.display.MovieClip;    import flash.events.*;     import flash.net.URLRequest;     import flash.system.ApplicationDomain;     import flash.system.LoaderContext; 	import flash.events.IOErrorEvent;    import flash.geom.Point;     public class Kocourek_Loader extends MovieClip{ 		public var CAT_LOADER:Loader; 		public var myGreeter;		private var cat_offset:Point = new Point(275.15, 199.95)				public function Kocourek_Loader():void{ 				trace("Init Kocourek Loader...")            			CAT_LOADER = new Loader();             var ldrContext:LoaderContext = new LoaderContext(false, ApplicationDomain.currentDomain);             CAT_LOADER.contentLoaderInfo.addEventListener(Event.COMPLETE, completeHandler); 			CAT_LOADER.contentLoaderInfo.addEventListener(IOErrorEvent.IO_ERROR, onError);            CAT_LOADER.load(new URLRequest("Kocourek.swf"), ldrContext);        };        public function completeHandler(event:Event):void         { 			var Greeter:Class = ApplicationDomain.currentDomain.getDefinition("classes.Master_Kocourek") as Class; 			myGreeter = Greeter(event.target.content); 			myGreeter.x-=cat_offset.x; myGreeter.y-=cat_offset.y; 			var catTMP = this.parent.getChildByName("mov_cat_temp");			catTMP.visible = false;			addChild(myGreeter);			//myGreeter.kocourkuUdelej("nene");        };		public function onError(e:IOErrorEvent):void{						trace("Unable to load:", e.text);			CAT_LOADER.contentLoaderInfo.removeEventListener(Event.COMPLETE, completeHandler); 			CAT_LOADER.contentLoaderInfo.removeEventListener(IOErrorEvent.IO_ERROR, onError);		};    } }//callvar cat_target = this.getChildByName("cat_target");cat_target.CAT_LOADER.contentLoaderInfo.addEventListener(Event.COMPLETE, catLoadComplete);function catLoadComplete(event:Event){    //trace("ok", event.target.content)    myGreeter = event.target.content; //get Kocourek ClassmyGreeter.kocourkuUdelej("ano");}