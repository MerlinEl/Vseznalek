Object.prototype.length = 1;Object.prototype.addItem = function(name:String, param:Object):Void  {	this[name] = param;	this.length++;};Object.prototype.removeItem = function(name:String):Boolean  {	var ret:Boolean = (delete this[name]);	if (ret) {		this.length--;	}	return ret;};var a = new Object();a.addItem("chia", new Number(55));trace( a.length);trace(a.removeItem("chia"));trace(a.length);