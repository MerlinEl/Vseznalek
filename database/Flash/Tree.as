function makeObjTree(arr:Array):Object{		var name, o = {}, result = o;	for (var i=0, iLen=arr.length; i<iLen; i++){		name = arr[i]				if (!o[name]){			o[name]={};			o = o[name];		}	}	return result;}function vriteInTreeTree(arr:Array, value:*):Object{		var name, o = {}, result = o;	for (var i=0, iLen=arr.length; i<iLen; i++){				name = arr[i]				if (!o[name]){						o[name]={};			o = o[name];		}		if (i==arr.length-1) o.value = value;	}	return result;}var names = ["bob","sue","dan"];var obj = vriteInTreeTree(names, "color");trace(obj)trace(obj.bob.sue.dan.value)