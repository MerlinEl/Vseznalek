import gra.ColorUtil;import gra.HEX;var size:Number = 20;var step:Number = 4;var max_colors = (size-1)*step+size;var columns:Number = 12;//num, min, max getColumns(max_colors, 6, 20);trace("max colors:"+max_colors)var colors = ColorUtil.getColorsBetween([	0xFFFFFF, step, 0xCCCCCC, step, 0x999999, step, 0x666666, step,	0x000000, step, 0x7A3F0A, step, 0xFF3714, step, 0xFF5E14, step,	0xFF9A14, step, 0xFFCB14, step, 0xFFFD14, step, 0xD0FF0F, step, 	0x0EFF0E, step, 0x00FFFF, step, 0x00AEAF, step, 0x1842FF, step, 	0x601CEC, step, 0xAF1EAD, step, 0xFE1552, step, 0xF986ED	])trace("colors:"+colors.length)ColorUtil.drawColors(canvas, colors, size, columns);var block = []var cnt = 8;for each (var c:uint in colors){		var hex:HEX = new HEX(c);	if (cnt == 0) {		cnt = 8;		trace(block+",")			block = [];	}	cnt--	block.push(" "+hex.toString());	//at end print last part	if (c == colors[colors.length-1]) trace(block)	}trace("max colors:"+max_colors)var colors = ColorUtil.getColorsBetween([	0xFFFFFF, step/2, 0xFFFF00, step, 0xFF0000, step, 0x0033FF, step/4, 0x006666, step, 	0xFFFF00, step, 0x333333, step/2, 0xFFFFFF, step, 0xFF0000, 0, 0xFFFFFF, step, 0x0033FF 	//0x0033FF, step, 0xFF0000, step, 0xFFFF00, step, 0x0099FF ])