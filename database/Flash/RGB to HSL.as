function rgb2hsl(rgb:RGB){	    var clrR = (rgb.red);    var clrG = (rgb.green);    var clrB = (rgb.blue);	trace("R:"+rgb.red+" G:"+rgb.green+" B:"+rgb.blue)    var clrMin = Math.min(clrR, clrG, clrB);    var clrMax = Math.max(clrR, clrG, clrB);    var deltaMax = clrMax - clrMin;    	var L, H, S	     L = (clrMax + clrMin) / 510;         if (0 == deltaMax){        H = 0;        S = 0;    }    else{        if (0.5 > L){            S = deltaMax / (clrMax + clrMin);        }        else{            S = deltaMax / (510 - clrMax - clrMin);        }        if (clrMax == clrR) {            H = (clrG - clrB) / (6.0 * deltaMax);        }        else if (clrMax == clrG) {            H = 1/3 + (clrB - clrR) / (6.0 * deltaMax);        }        else {            H = 2 / 3 + (clrR - clrG) / (6.0 * deltaMax);        }        if (0 > H) H += 1;        if (1 < H) H -= 1;    }    return [H, S, L];}