//Copy Filtersfl.outputPanel.clear(); //clear consolevar doc = fl.getDocumentDOM();var sel  = doc.selection;if (doc != null && sel.length > 1) {	var my_filters = doc.getFilters(); 	//fl.trace("On selection1:"+sel[0].name+" Filters found: "+my_filters.length);	fl.trace(sel[1].name)	fl.getDocumentDOM().setFilters(my_filters);}var myFilters = fl.getDocumentDOM().getFilters();var filterName = "blurFilter";// if there is no blurFilter, add oneif (!test(myFilters,filterName) ){	fl.getDocumentDOM().addFilter(filterName);}// update filters listmyFilters = fl.getDocumentDOM().getFilters();for(var i=0; i < myFilters.length; i++){     if(myFilters[i].name == filterName)	{         myFilters[i].blurX = 10;         myFilters[i].blurY = 10;        myFilters[i].quality = 'high';    } }fl.getDocumentDOM().setFilters(myFilters);