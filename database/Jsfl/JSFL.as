//soubot Source.xml<?xml version="1.0"?><dialog id="SetLinkageURL" title="Set Linkage URL" buttons="accept, cancel">	<vbox>		<hbox>			<label value="Set the shared library import source on multiple items at once" />		</hbox>		<hbox>			<label value="Written by David Johston, Trilogy Studios, April 2009" />		</hbox>		<separator/>		<hbox>			<choosefile id="choosefileControl" type="open" pathtype="absolute" size="50"/>		</hbox>	</vbox></dialog>//JSFL skript/******************************************************************************************	Set Import Source 	By David Johnston	Set which source file the selected symbols should draw from when they are updated	Last Modified 10:28 AM Thursday, January 14, 2010******************************************************************************************/SetLinkageURL = function(){	}var proto = SetLinkageURL.prototype;proto.doc = fl.getDocumentDOM();proto.init = function(){	if (xmui.dismiss == "accept")	{			if (!this.doc)		{			alert ("You need to open a Flash File First");		}		else		{			this.lib = this.doc.library;			var cItems = this.lib.getSelectedItems();			var cLength = cItems.length;						for(var i = 0; i < cLength; i++)			{				// check itemType (folders should not be included):				var iType = this.lib.getItemType(cItems[i].name);				if (iType == "graphic" || iType == "movie clip")				{					// select the current item in the library:					this.lib.selectItem(cItems[i].name);										path_uri = xmui.choosefileControl;					source_name = cItems[i].name;					// Z:\Art\Kung_Fu_Panda\avatars\tiger\male\base\tiger_male_base.fla					fl.trace("new source path: " + xmui.choosefileControl);					this.lib.setItemProperty('sourceFilePath', path_uri);					this.lib.setItemProperty('sourceLibraryName', source_name);				}			}		}	}}var theDoc = fl.getDocumentDOM();if (theDoc.library.getSelectedItems().length < 1){	alert("Please select at least one item in the library!");}else{	var xmui = theDoc.xmlPanel(fl.configURI + "Commands/Source.xml");	if (xmui.dismiss == "accept")	{		var obj = new SetLinkageURL();		obj.init();	}}