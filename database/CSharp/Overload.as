    public class Point3 { //not tested not used        public static float x = 0;        public static float y = 0;        public static float z = 0;        public Point3() : this(0, 0, 0) { }        public Point3(float X, float Y, float Z) {            x = X;            y = Y;            z = Z;        }        public float length() {            float sq = ( x * x ) + ( y * y ) + ( z * z );            return sq;        }    }