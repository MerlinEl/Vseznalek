try (destroyDialog  dn_drawing_example) catch()rollout dn_drawing_example "" width:500 height:500(	dotNetControl pBox "pictureBox" pos:[5,5] width:490 height:490	fn pBoxPaint arg =	(				g=arg.graphics				fillBrush=dotNetObject "system.drawing.solidBrush" (dotNetClass "system.drawing.color").white		outLinePen=dotNetObject "system.drawing.pen" (dotNetClass "system.drawing.color").black 3			g.TextRenderingHint = g.TextRenderingHint.AntiAlias		g.SmoothingMode = g.SmoothingMode.AntiAlias						graphic2D = dotNetobject "System.Drawing.Drawing2D.GraphicsPath"		------------------------------------------------		string_text = "test"		font_family =  dotNetObject "System.Drawing.FontFamily" "Arial Black"		font_style = 1				font_size = 50		startPoint = dotNetObject "system.drawing.point" 100 100 				font_format = (dotNetclass "System.Drawing.StringFormat").GenericTypographic		graphic2D.AddString string_text font_family font_style font_size startPoint font_format		------------------------------------------------		g.FillPath fillBrush graphic2D		g.DrawPath outLinePen graphic2D				)		on dn_drawing_example open do 	(		pBox.backColor=(dotNetClass "system.drawing.color").gray	)		on pBox paint args do pBoxPaint args)createDialog dn_drawing_example