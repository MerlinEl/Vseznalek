fn setupPsdDDSList settings_ini = (				if mcAssetGeneratorDDSDialog != undefined do DestroyDialog mcAssetGeneratorDDSDialog		rollout mcAssetGeneratorDDSDialog "DDS Export Setup:" width:172 height:252		(			local SettingsINI = ""			button 'btn_accept' "Accept" pos:[108,220] width:49 height:21 align:#left			button 'btn_cancel' "Cancel" pos:[12,220] width:48 height:21 align:#left			dotNetControl 'clb_mods' "CheckedListBox" pos:[12,24] width:144 height:191 align:#left 			groupBox 'grp6' "Active List:" pos:[4,4] width:164 height:244 align:#left			fn loadDDSTexturesSettings = (								if clb_mods.Items.count != 0 do clb_mods.Items.Clear()				local folder_names = getINISetting SettingsINI "DDS_Textures"				for item_name in folder_names do (										local is_checked = (getINISetting SettingsINI "DDS_Textures" item_name) == "true"					clb_mods.Items.Add item_name is_checked				) 			)			fn savePsdFoldersSettings = (				for i=0 to clb_mods.Items.count-1 do (										local is_checked = (clb_mods.GetItemChecked i) as string					local item_name = clb_mods.items.item[ i ]					setINISetting SettingsINI "DDS_Textures" item_name is_checked				)				DestroyDialog  mcAssetGeneratorDDSDialog			)			on mcAssetGeneratorDDSDialog open do (								clb_mods.BackColor = (dotNetClass "System.Drawing.Color").FromArgb 40 40 40				clb_mods.ForeColor = (dotNetClass "System.Drawing.Color").FromArgb 200 200 200				clb_mods.BorderStyle = (dotNetClass "BorderStyle").FixedSingle				clb_mods.HorizontalScrollbar = clb_mods.MultiColumn = false ; clb_mods.CheckOnClick = true			)			on btn_accept pressed do savePsdFoldersSettings()			on btn_cancel pressed do DestroyDialog  mcAssetGeneratorDDSDialog		)		CreateDialog mcAssetGeneratorDDSDialog  pos:[mouse.screenpos.x, mouse.screenpos.y - 252 / 2]		mcAssetGeneratorDDSDialog.SettingsINI = settings_ini		mcAssetGeneratorDDSDialog.loadDDSTexturesSettings()	)--INI--[DDS_Textures]diffuse=truedetail_mask=falseemissive=trueglossiness=falsenormal=true