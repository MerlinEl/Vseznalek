try destroydialog GroupCreator catch()fn getuserstring default:"Groupname" =(	RenameDialog = dotNetObject "MaxCustomControls.RenameInstanceDialog" default	DialogResult = RenameDialog.Showmodal()	dotnet.compareenums RenameDialog.DialogResult ((dotnetclass "System.Windows.Forms.DialogResult").OK)	Outputstring = RenameDialog.InstanceName	Outputstring)rollout GroupCreator "---"(	local mygroup		button BTN_MakeGroup "Make Group"		on BTN_MakeGroup pressed do	(		if mygroup == undefined then		(			mygroup = group selection name:(getuserstring default:"Groupname") select:true		)		else select mygroup	))createdialog GroupCreator