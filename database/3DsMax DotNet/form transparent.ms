(	fn closeForm s a = (s.parent.close() )		fn changeBackcolor s a = (		s.backColor = mcDotnet.dColor red		s.TransparencyKey = s.BackColor	)	local font1 = dotnetobject "System.Drawing.Font" "Impact" 20	local font2 = dotnetobject "System.Drawing.Font" "Impact" 12			local btn_save = dotnetobject "Button"	btn_save.text = "BACKUP NOW"	btn_save.font = font2	btn_save.foreColor = mcDotnet.dColor (color 9 102 51)	btn_save.backColor = mcDotnet.dColor green	btn_save.Dock = btn_save.Dock.Top		local btn_cancel = dotnetobject "Button"	btn_cancel.text = "ABORT"	btn_cancel.font = font2	btn_cancel.foreColor = mcDotnet.dColor  (color 9 102 51)	btn_cancel.backColor = mcDotnet.dColor green	btn_cancel.Dock = btn_cancel.Dock.Bottom	local lb=dotNetObject "label"	lb.text = "Autosave After\n 5"	lb.font = font1	lb.TextAlign = lb.TextAlign.MiddleCenter	lb.width = 200	lb.height = 200	--lb.autoSize = true	lb.foreColor=mcDotnet.dColor yellow	lb.backColor=mcDotnet.dColor red	--center text	lb.Location = mcDotnet.dPoint [form.Width / 2 - lb.Width / 2, form.Height / 2 - lb.Height / 2]		local form =  dotNetObject "MaxCustomControls.MaxForm"	form.StartPosition = form.StartPosition.Manual	form.ShowInTaskBar = false	form.FormBorderStyle = form.FormBorderStyle.None	form.Controls.addrange #(btn_save, btn_cancel, lb)	form.StartPosition = form.StartPosition.Manual	form.location = mcDotnet.dPoint (mcSystem.getMaxCenter() - [form.width, form.height] / 2)	dotNet.addEventHandler btn_save "MouseUp" closeForm	dotNet.addEventHandler btn_cancel "MouseUp" closeForm	dotnet.AddEventHandler form "BackColorChanged" changeBackcolor	form.showmodeless() )