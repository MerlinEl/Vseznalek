(        fn SetViewportsRedraw state =    (            WM_SETREDRAW = 11    --0x000B                for j in (windows.getchildrenhwnd #max) where j[4] == "ViewPanel" do        (            panel = j[1]            case state of            (                true:                (                    windows.sendmessage panel WM_SETREDRAW 1 0                    completeredraw()                )                false: windows.sendmessage panel WM_SETREDRAW 0 0            )            exit        )    )    SetViewportsRedraw on    )