      XMLDocument.setProperty('SelectionLanguage', 'XPath');      XMLNodeList := XMLDocument.SelectNodes(APath);      MsgBox('XMLNodeList: ' + ExpandConstant(XMLNodeList.length), mbInformation, MB_OK);      XMLNode := XMLDocument.SelectSingleNode(APath)      MsgBox('XMLNode: ' + ExpandConstant(IDispatch(XMLNode)), mbInformation, MB_OK);      for Index := 0 to XMLNode.length - 1 do           begin        XMLNode := XMLNodeList.item[Index];       MsgBox('Node: ' + ExpandConstant(Index), mbInformation, MB_OK);        Log(          Format('Name = %s; Currency = %s', [           XMLNode.SelectSingleNode('name').Text,            XMLNode.SelectSingleNode('suggestedCurrency').Text]));       end;