function GetInstallDirs(const rootTargetPath: string): Array of String; var  findRec: TFindRec;  bookDir: string;  targetFile: string;	dirPathArr : Array of String;	i: integer; begin  //MsgBox('Search directories in Root dir: [ '+rootTargetPath + '\* ]', mbInformation, MB_OK);		i := 0;    if FindFirst(rootTargetPath + '\*', findRec) then        try        repeat					bookDir := findRec.Name;					if ( bookDir <> '.') and (bookDir <> '..') then					begin						if findRec.Attributes and FILE_ATTRIBUTE_DIRECTORY = 0 						then //Get files							//Log('Found file:' + bookDir);						else //Get directories							begin							//Log('Found dir:' + bookDir);							targetFile := rootTargetPath + '\' + bookDir + '\InteligntFrame.swf';							Log('fpath:' + targetFile);							SetArrayLength(dirPathArr , i+1); //add new slot in to array							dirPathArr [i] := targetFile; //append foder path in to array							i := i + 1;							end;					end;        until not FindNext(findRec);        finally            FindClose(findRec);						Result := dirPathArr;        end;end;