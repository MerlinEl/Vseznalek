function getHourStr() {  var today = new Date();  var h = checkTime(today.getHours());  var m = checkTime(today.getMinutes());  var s = checkTime(today.getSeconds());  return h + m + s;}// 0803function getTimeStr() {  var today = new Date();  var y = checkTime(today.getFullYear());  var m = checkTime(today.getMonth() + 1);  var d = checkTime(today.getDate());  return m + d;}function checkTime(i) {  if (i < 10) i = "0" + i;  return i.toString();}