<grid>		<columns>			<column/>					<label value="Clones Count:" />				<textbox id="edt_clones" value="1" prompt="count" width="130" onchange="updateResult()"/>				<label value="Clones Offset:" />				<textbox id="edt_offset" value="0" prompt="offset" width="130" onchange="updateResult()"/>				<label value="Absolute - Relative:" />				<checkbox checked="false"  label="From center"  id="ckb_center" width="80"  onpick=""/>				<label value="Rotate:" />				<textbox id="edt_angle" value="0" prompt="angle" width="130" onchange="updateResult()" oncreate="init()"/>		</columns>		<separator/>		<rows>			<row align="center">				<vbox>					<hbox>						<spacer/>						<button oncommand="cloneObject('up')"  id="btn_up"  label="↑" width="40" />						<spacer/>					</hbox>										<hbox>						<button oncommand="cloneObject('le')"  id="btn_left"  label="←" width="40" />						<button oncommand="cloneObject('ce')"  id="btn_centr"  label="♦" width="40" />						<button oncommand="cloneObject('ri')"  id="btn_right"  label="→" width="40" />					</hbox>										<hbox>						<button  id="btn_a"  label="" width="40" />						<button oncommand="cloneObject('do')"  id="btn_down"  label="↓" width="40" />						<button  id="btn_b"  label="" width="40" />					</hbox>				</vbox>			</row>			<button oncommand="deleteInstancesBuffer()"  id="btn_del"  label="Del" width="40" />			<separator/>		</rows>	</grid>