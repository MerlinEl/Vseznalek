var dir = fl.browseForFolderURL("select fla folder");//open a foldervar files = FLfile.listFolder(dir,"files");//get the files (note: NOT recursive!)var filesNum = files.length;fl.outputPanel.clear();for(var i = 0 ; i < filesNum; i++){    if(files[i].substr(files[i].lastIndexOf(".")+1) == 'fla'){//look for fla's        var doc = fl.openDocument(dir+'/'+files[i]);        //do whatever to your document here        doc.selectAll();        doc.group();        doc.align('top', true);        doc.align('left', true);        doc.unGroup();        doc.selectNone();        fl.saveDocument(doc);        fl.closeDocument(doc,false);        fl.trace(files[i]+' done');    }}fl.trace('all done!');