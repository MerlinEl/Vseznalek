var ptArray = [];var doneEdge = [];sel = fl.getDocumentDOM().selection;for (var n = 0; n < sel.length; n++) {	exportString += 'shapeArrayz['+n+'] = [';	selectionNumber = sel.length;	var elt = sel[n];	if (elt.elementType != 'shape') {		continue;	}	elt.beginEdit();	for (i=0; i<elt.contours.length; i++) {		var cont = elt.contours[i];		var he = cont.getHalfEdge();		var startId = he.id;		var id = 0;		while (id != startId) {			var ed = he.getEdge();			if (!isDrawn(ed.id)) {				doneEdge.push(ed.id);				for (var j = 0; j<3; j++) {					var pt = ed.getControl(j);					ptArray.push(pt.x, pt.y , j);					exportString += '[' + pt.x + ',' + pt.y + ',' + j + '] , ';				}			}			he = he.getNext();			id = he.id;		}	}	elt.endEdit();	exportString += '];\n';}// fl.trace(ptArray);// fl.trace("// end ---------------------------");