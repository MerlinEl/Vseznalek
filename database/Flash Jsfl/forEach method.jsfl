// Add foreach method in to ArrayArray.prototype.forEach = function (callback, thisArg){		if (this == null) { throw new TypeError('Array.prototype.forEach called on null or undefined'); }	var T, k;	var o = Object(this);	var len = o.length >>> 0;	if (typeof(callback) !== "function") { throw new TypeError(callback + ' is not a function'); }	if (arguments.length > 1) { T = thisArg; }	k = 0;	while (k < len) {      var kValue;      if (k in o) {        kValue = o[k];        callback.call(T, kValue, k, o);      }      k++;    }}	["a", "b", "c", "d"].forEach(function(letter, index, array) {			trace("letter:" + letter);	})	["a", "b", "c", "d"].forEach(function(fname, index, array) {			var fpath = Core_Dir + fname + ".jsfl";		trace("Register Class:" + fpath);		fl.runScript(fpath);	})	["a", "b", "c", "d"].forEach(function(fname) {			var fpath = Core_Dir + fname + ".jsfl";		trace("Register Class:" + fname);		fl.runScript(fpath);	})