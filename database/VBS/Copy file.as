Const ForReading = 1Const OverwriteExisting = TRUErootDir = "../../@NS INTERACTIVE/"outputDir = "CD/Output/"dataDir = "CD/Output/data"srcFile = "Smazat regiztraci z localization.vbs"Set objFSO = CreateObject("Scripting.FileSystemObject")Set objFile = objFSO.OpenTextFile("_CD_paths.txt")'appDir = objFSO.GetParentFolderName(objFile)Do Until objFile.AtEndOfStream	Dim bookDir    bookDir = objFile.ReadLine	Dim destFile	destFile = rootDir & bookDir & outputDir & srcFileWscript.Echo "Folder exists?"& isFolderExists(rootDir & bookDir & outputDir)Wscript.Echo "File exists?"& isFileExists(destFile)    objFSO.CopyFile srcFile, destFile, OverwriteExistingLoopFunction isFolderExists(dir)   Dim fso, msg   Set fso = CreateObject("Scripting.FileSystemObject")   If (fso.FolderExists(dir)) Then      msg = dir & " true."   Else      msg = dir & " false"   End If   isFolderExists = msgEnd FunctionFunction isFileExists(fpath)   Dim fso, msg   Set fso = CreateObject("Scripting.FileSystemObject")   If (fso.FileExists(fpath)) Then      msg = fpath & " true."   Else      msg = fpath & " false"   End If   isFileExists = msgEnd Function