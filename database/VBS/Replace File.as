'================================================================'rename painter.bk to painter.swf and vice versa'================================================================Dim fso, xSet fso = WScript.CreateObject("Scripting.FileSystemObject")'fso.MoveFile "painter.swf", "painter.bk1"'fso.MoveFile "painter.bk", "painter.swf"'fso.MoveFile "painter.bk1", "painter.bk"'Remove current fileIf (fso.FileExists("painter.swf")) Then fso.DeleteFile("painter.swf")'Copy old or new file at his placex=msgbox("Pouzit novu verzi?" ,4, "Question:")If x = vbNo Then	fso.CopyFile "painter.old", "painter.swf"	MsgBox "Stara verze je pripravena k pouziti.", 4096, "Ne:"Else	fso.CopyFile "painter.new", "painter.swf"	MsgBox "Nova verze je pripravena k pouziti.", 4096, "Ano:"End IfWscript.Quit(0)