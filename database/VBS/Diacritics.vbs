Function RemoveDiacritics(InputStr)  'WScript.Echo "InputStr:" & InputStr  Dim cs, us, retval, out_test  cs = "áéěíóúůýžščřďťňŽŠČŘĎŤŇÁÉĚÍÓÚŮÝ"  us = "aeeiouuyzscrdtnZSCRDTNAEEIOUUY"  Dim i, s, pos  out_test = ""  For i = 1 To Len(InputStr)    s = Mid(InputStr, i, 1)    pos = InStr(1, cs, s)	out_test = out_test & "ch:" & s & "(" & pos & ") | "     If pos > 0 Then      retval = retval & Mid(us, pos, 1)    Else      retval = retval & s    End If  Next   'WScript.Echo "out_test:" & out_test  RemoveDiacritics = retvalEnd FunctionFunction AsciiToUTF8(InputStr)	Dim objStream: Set objStream = CreateObject( "ADODB.Stream" )	objStream.Open	objStream.Type = 2	objStream.Charset = "Windows-1252"	objStream.WriteText(InputStr)	objStream.Position = 0	objStream.Charset = "UTF-8"	AsciiToUTF8 = objStream.ReadText	Call objStream.Close(): Set objStream = NothingEnd FunctionFunction RemoveDiacritics(InputStr)	Dim charsFrom, charsTo, current_char, found_index, outString	charsFrom = "ÀÁÂÃÄÅÇÈÉÊËÌÍÎÏÑÒÓÔÕÖÙÚÛÜÝßàáâãäåçèéêëìíîïñòóôõöùúûüýÿĀāĂăĄąĆćĈĉĊċČčĎďĐđĒēĔĕĖėĘęĚěĜĝĞğĠġĢģĤĥĦħĨĩĪīĬĭĮįİıĶķĸĹĺĻļĽľĿŀŁłŃńŅņŇňŉŊŋŌōŎŏŐőŔŕŖŗŘřŚśŜŝŞşŠšŢţŤťŦŧŨũŪūŬŭŮůŰűŲųŴŵŶŷŸŹźŻżŽžſ"	charsTo   = "AAAAAACEEEEIIIINOOOOOUUUUYsaaaaaaceeeeiiiinooooouuuuyyAaAaAaCcCcCcCcDdDdEeEeEeEeEeGgGgGgGgHhHhIiIiIiIiIiKkkLlLlLlLlLlNnNnNnNnNOoOoOoRrRrRrSsSsSsSsTtTtTtUuUuUuUuUuUuWwYyYZzZzZzs"	outString = ""	WScript.Echo "InputStr:" & InputStr	For i=1 To Len(InputStr)				current_char = Mid(InputStr,i,1)		found_index = InStr(1, charsFrom, current_char)		'WScript.Echo "current_char:" & current_char & " found_index:" & found_index		If found_index > 0 Then 					outString = outString & Mid(charsTo,found_index,1)		Else 			outString = outString & current_char		End If	Next	RemoveDiacritics = outStringEnd Function