private var _theyArePressed:Object = { };stage.addEventListener(KeyboardEvent.KEY_DOWN, onKeyPress);		stage.addEventListener(KeyboardEvent.KEY_DOWN, onKeyLeave);		this.stage.focus = this;this.focusRect = false;private function onKeyPress(e:KeyboardEvent):void {		_theyArePressed[e.keyCode] = true;	// combo check	if (_theyArePressed[Keyboard.SPACE] && _theyArePressed[Keyboard.UP]) {		//do anything	}}private function onKeyLeave(e:KeyboardEvent):void {		_theyArePressed[e.keyCode] = false;}