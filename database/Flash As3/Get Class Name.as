		private function clonePiece(piece:SimpleFadeInOut):SimpleFadeInOut {						var piece_class_name:String = getQualifiedClassName(piece);			//ctrace("piece:{0} piece_class_name:{1}", piece, piece_class_name);			switch (piece_class_name) {								case "CubePiece"  : return new CubePiece();				case "PrismPieceF": return new PrismPieceF();				case "PrismPieceB": return new PrismPieceB();				case "PrismPieceL": return new PrismPieceL();				case "PrismPieceR": return new PrismPieceR();			}			return null;		}var piece_z:SimpleFadeInOut = clonePiece(piece);