var comp:List = new List();comp.addEventListener(Event.ADDED_TO_STAGE, init);addChild(comp);function init(e:Event):void{	ctrace("init")	comp.removeEventListener(Event.ADDED_TO_STAGE, init);	comp.setStyle("cellRenderer", CustomCellRenderer); 	comp.addItem({label:"Burger", price:"$5.95"}); 	comp.addItem({label:"Fries", price:"$1.95"});	comp.addItem({label:"Burger", price:"$5.95"}); 	comp.addItem({label:"Fries", price:"$1.95"}); comp.addEventListener(MouseEvent.CLICK, hackMouseListener, true);}function hackMouseListener(e:MouseEvent):void{    e.ctrlKey = true;}package {	import fl.controls.CheckBox;	import fl.controls.listClasses.ICellRenderer;	import fl.controls.listClasses.ListData;	import fl.controls.List;		public class CustomCellRenderer extends CheckBox implements ICellRenderer {		private var _listData:ListData;		private var _data:Object;				public function CustomCellRenderer() {		}				public function set data(d:Object):void {						_data = d;			label = d.label;		}				public function get data():Object {						return _data;		}				public function set listData(ld:ListData):void {						_listData = ld;		}				public function get listData():ListData {						return _listData;		}				public override function set selected(val_bool:Boolean):void {						_selected = val_bool			super.selected = _selected;		}				public override function get selected():Boolean {						return _selected;		}	}}