var used = new Array();function getArrayCombinations(arr){		var _result={};	function nextItem(arr,l,key,used){			if(!key) return;		for(var i=0;i<l;i++){					if(used.indexOf(""+i)<0){							_result[key+"+"+arr[i]]="";				nextItem(arr,l,key+"+"+arr[i],used+i);			}		}	}	var l=arr.length;	for (var i=0; i<arr.length; i++) {			//trace("loop:"+i)		_result[arr[i]] = "";		nextItem(arr, l, arr[i], ""+i)	}	return _result;}var obj_list = getArrayCombinations([10,5,2,1]);var cnt=0;for (var o in obj_list){		trace(":"+o)	cnt++}trace ("Combinations found:"+cnt)