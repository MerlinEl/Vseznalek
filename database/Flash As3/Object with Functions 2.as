import flash.geom.Point;var markers:Array = [new Point(10, 20), new Point(30, 40)]var points:Object = {		lp:markers[0],	rp:markers[1],	sortByX:function(){				if (this.lp.x > this.rp.x) this.swapPoints();	},	swapPoints:function(){				var sp:Point = this.lp;		this.lp = this.rp;		this.rp = sp;	},	toString:function(){				return "Object{left point:" + this.lp + " right point:" + this.rp + "}";	}}trace(points);points.sortByX();trace(points);