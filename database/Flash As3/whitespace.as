String.prototype.trim = String.prototype.trim || function () {    return this.replace(/^\s+|\s+$/g, "");};String.prototype.trimLeft = String.prototype.trimLeft || function () {    return this.replace(/^\s+/, "");};String.prototype.trimRight = String.prototype.trimRight || function () {    return this.replace(/\s+$/, "");};String.prototype.trimFull = String.prototype.trimFull || function () {    return this.replace(/(?:(?:^|\n)\s+|\s+(?:$|\n))/g, "").replace(/\s+/g, " ");};This only removes spaces; there are no regular expressions:function trim(str){    return str.split(' ').join();}