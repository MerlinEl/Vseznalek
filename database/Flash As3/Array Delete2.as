function slowRemove(list:Array, item:*):void {    var index:int = list.indexOf(item);        if (index >= 0) list.splice(index, 1);}function fastRemove(list:Array, item:*):void {    var index:int = list.indexOf(item);    if (index >= 0) {        if (index === list.length - 1) list.pop();        else {            // Replace item to delete with last item.            list[index] = list.pop();        }    }}