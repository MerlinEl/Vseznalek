var used = new Array();function getWordsNew(masterword){		var _result={};	var a,i,l;	function nextLetter(a,l,key,used){			var i, j;		if(key.length==l) return;		for(i=0;i<l;i++){					if(used.indexOf(""+i)<0){							_result[key+a[i]]="";				nextLetter(a,l,key+a[i],used+i);			}		}	}	a=masterword.split("");	l=a.length;	for (i = 0; i < a.length; i++) {			_result[a[i]] = "";		nextLetter(a, l, a[i], "" + i)	}	return _result;}var re = getWordsNew("abcd")var cnt=0;for (var o in re){		trace(":"+o)	cnt++}trace ("Combinations found:"+cnt)