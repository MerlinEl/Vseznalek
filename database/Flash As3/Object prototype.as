//working examplevar a = new Object();trace( a.chia); //undefineda.chia = 55;trace( a.chia); //55a.ria = 22trace("chia:" + a.chia + " ria:" + a.ria) //chia:55 ria:22a.swap = function(){		var a = this.chia;	this.chia = this.ria;	this.ria = a;}a.swap();trace("chia:" + a.chia + " ria:" + a.ria) //chia:22 ria:55//otherObject.prototype.length = 1;Object.prototype.addItem = function(name:String, param:Object):Void  {	this[name] = param;	this.length++;};Object.prototype.removeItem = function(name:String):Boolean  {	var ret:Boolean = (delete this[name]);	if (ret) {		this.length--;	}	return ret;};