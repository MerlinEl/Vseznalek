		private function getClosedCage(cage:MovieClip, arr:Array):MovieClip{						var distances = new Array();			for each (var c in arr){				var dist = Math.abs(c.y-cage.y);				distances.push(dist);			}			var lower_dist = 100000;			var neighbour_cage;			for (var i=0; i<distances.length;i++) {								var d=distances[i];				if (d<lower_dist){					lower_dist=d;					neighbour_cage=arr[i];				}			}			return neighbour_cage;		};