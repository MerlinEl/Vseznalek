		public function intersects(other:MeshObject):Boolean		{				// Use up to 6 separating planes			if ( this.model.maxX + this.model.x < other.model.minX + other.model.x )				return false;			if ( this.model.maxY + this.model.y < other.model.minY + other.model.y ) 				return false;			if ( this.model.maxZ + this.model.z < other.model.minZ + other.model.z )				return false;   			if ( this.model.minX + this.model.x > other.model.maxX + other.model.x )   				return false;   			if ( this.model.minY + this.model.y > other.model.maxY + other.model.y )   				return false;   			if ( this.model.minZ + this.model.z > other.model.maxZ + other.model.z)				return false;			// otherwise, must be intersecting			return true;		}				public function collision(other:MeshObject):void		{				}