@echo off Set COUNTER=0Set CYCLES=40tasklist /FI "IMAGENAME eq FlashDevelop.exe" | find /I /N "FlashDevelop.exe" > nulIF %ERRORLEVEL% equ 0 goto EDITFILEIF %ERRORLEVEL% equ 1 goto RUNAPP  :EDITFILEset /A COUNTER=COUNTER+1if %COUNTER% EQU %CYCLES% goto ENDECHO Executing FlashDevelop...tasklist /FI "IMAGENAME eq FlashDevelop.exe" | find /I /N "FlashDevelop.exe" > nulIF %ERRORLEVEL% equ 1 goto EDITFILEECHO FlashDevelop is runninggoto END:RUNAPPECHO FlashDevelop is not running"E:\Aprog\FlashDevelop\FlashDevelop.exe"goto EDITFILE:ENDECHO Process TERMINATED.pause>nul