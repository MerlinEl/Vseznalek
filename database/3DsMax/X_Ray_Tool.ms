tool xRay(	local scanSel, rf=newrolloutfloater "X-Ray" 160 200	on start do (				rollout scanSel "Selection"	(						listbox oToSel "Objects:" items:#()			on oToSel selected nameIndex do select (getNodeByName oToSel.items) 			on oTosel doubleClicked itm do (								removerollout scanSel				closeRolloutFloater rf			)		)		addRollout scanSel rf	)		fn getObjectsInRay vpt = (				for o in geometry where (						intersectRay o (mapScreenToWorldRay vpt)				) != undefined collect o.name	)		on freeMove do  (		-- 		rf.pos = viewPoint		scanSel.oToSel.items = getObjectsInRay viewPoint	)	on mousePoint clickno do (				stopTool xRay		closeRolloutFloater rf	)	on mouseAbort clickno do (				stopTool xRay		closeRolloutFloater rf	))startTool xRay