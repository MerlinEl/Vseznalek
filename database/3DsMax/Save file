"Save To Version 2014"src_fpath = maxFilePath + maxFileNametrgt_fpath = micra.RootDir + maxFileNameas_version  = 2014_result = saveMaxFile trgt_fpath saveAsVersion:as_version useNewFile:falseif(_result) then format "SaveToVersion(M%): successfully saved %\n" as_version trgt_fpathelse format "SaveToVersion(M%): failed to save \"%\"\n" as_version trgt_fpathfunction SaveToVersion version Filename:(maxfilepath+maxfilename) quiet:true =(	local _result = false		local oldest = (maxversion())[1] / 1000 - 3			if oldest < 12 then oldest = 12		local validVersions = for i = oldest to (maxversion())[1] / 1000 collect 1998 + i			--- if the scene does'nt have a filenamee already, enforce a standard save now	if( GetFileNameFile Filename == "" ) then	(		MessageBox "Current Scene is still unnamed.\nPlease save the Scene and retry." title:"SaveToVersion"	)				else	(		if( findItem validVersions version != 0 ) then		(			local saveFilename = getFilenamePath Filename + getFilenameFile Filename + getFilenameType FileName						if( quiet OR not (doesfileexist saveFileName)) OR querybox ("Warning: File '" + saveFileName as string + "'\n alrady exists. Overwrite?") then			(				try				(									_result = saveMaxFile saveFileName saveAsVersion:version useNewFile:false					if(_result) then format "SaveToVersion(M%): successfully saved %\n" version saveFileName					else format "SaveToVersion(M%): failed to save \"%\"\n" version saveFileName				)				catch( format "SaveToVersion(M%): failed to save \"%\" :%\n" version saveFileName (getCurrentException()))			)					)		else MessageBox("Invalid Target Version:\n\t" + version as string) title:"SaveToVersion"	)	--_result)macroScript SaveToVersioncategory:"LS3D Engine"tooltip:"Save To Version 2014"buttonText:"Save To Version 2014"Icon:#("Maxscript", 3)(	on execute do SaveToVersion 2014)