mapChannels = polyop.getNumMaps (getNodeByName (newVertsSourceMeshName as string))polyop.setNumMaps obj mapChannels keep:falsefor channel = 0 to (mapChannels-1) do(	--mapSupport = polyop.getMapSupport (getNodeByName (newVertsSourceMeshName as string)) channel	polyop.setMapSupport obj channel true	polyop.setMapSupport (getNodeByName (newVertsSourceMeshName as string)) channel true	mapVerts = polyop.getNumMapVerts (getNodeByName (newVertsSourceMeshName as string)) channel	polyop.setNumMapVerts obj channel mapVerts keep:false	mapFaces = polyop.getNumMapFaces (getNodeByName (newVertsSourceMeshName as string)) channel	polyop.setNumMapFaces obj channel mapFaces keep:false	for vertNr = 1 to mapVerts do	(		in coordsys local tmpLocation = polyop.getMapVert (getNodeByName (newVertsSourceMeshName as string)) channel vertNr		polyop.setMapVert obj channel vertNr tmpLocation	)	for faceNr = 1 to mapFaces do	(		tmpVertArray = polyop.getMapFace (getNodeByName (newVertsSourceMeshName as string)) channel faceNr		polyop.setMapFace obj channel faceNr tmpVertArray	)))