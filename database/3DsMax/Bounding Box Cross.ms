(	local local_bbox = nodeLocalBoundingBox $ --(required AvGuard Extension)	local tm = $.transform	local bmin = local_bbox[1]*(inverse tm) 	local bmax = local_bbox[2]*(inverse tm)	local bbox_size = local_bbox[2] - local_bbox[1] -- w l h	local center = (bmin + bmax)/2	local p0 = center --center red	local p1 = [center.x,center.y,bmin.z] --bottom green	local p2 = [center.x,center.y,bmax.z] --top blue	local p3 = [center.x,bmin.y,center.z] --front yellow	local p4 = [center.x,bmax.y,center.z] --back gray	local p5 = [bmin.x,center.y,center.z] --left white	local p6 = [bmax.x,center.y,center.z] --right brown	local points = #(p0, p1, p2, p3, p4, p5, p6)	local colors = #(red, green, blue, yellow, gray, white, brown)	global geo_points = #()	for i=1 to points.count do append geo_points (point pos:(points[i]*$.transform) wirecolor:colors[i]))/*delete geo_points*/