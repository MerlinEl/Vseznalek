local task_progress = falsefunction PerformDelayedTasks s a = (		task_progress = true	s.enabled = false -- important: don't repeat this timer event	dotnet.RemoveEventHandler s "Elapsed" PerformDelayedTasks --for sanity and cleanup        	-- put all your tasks below   	$.vertexColorType = 2 --#alpha	update $		task_progress = false	)obj.vertexColorType = 0 -- #colorupdate objif not task_progress do (		local delayTimer= dotnetobject "Windows.Forms.Timer"	delayTimer.Interval=200	dotnet.AddEventHandler delayTimer "Tick" PerformDelayedTasks	delayTimer.enabled = true)	