fn createDialogAtAppCenter dia = (    local app_pos = getDialogPos ::gLS3D_MaterialLibraryDesignerUI    local app_size = getDialogSize ::gLS3D_MaterialLibraryDesignerUI    local dia_size = getDialogSize dia    local app_center_pos = [        app_pos.x + (app_size.x / 2)  - (dia_size.x / 2),         app_pos.y + (app_size.y / 2)  - (dia_size.y / 2) \    ]    CreateDialog dia pos:app_center_pos)createDialogAtAppCenter rMatLibSetUVWrollout rMatLibSetUVW "Set UV:" width:112 height:124(    spinner 'spnU' "U" pos:[12,22] width:81 height:16 align:#left    spinner 'spnV' "V" pos:[12,41] width:81 height:16 range:[0,100,0] align:#left    button 'BTNOK' "ok" pos:[12,90] width:88 height:21 align:#left    pickbutton 'BTNPICKFROMOBJECT' "Pick From Object" pos:[12,66] width:88 height:21 align:#left    GroupBox 'grp1' "Tiling:" pos:[4,4] width:104 height:114 align:#left        local ExtButton = undefined        on BTNOK pressed do    (        if ExtButton != undefined then        (            local tagValues = #(spnU.value, spnV.value)            ExtButton.Tag = tagValues            ExtButton.Value = "uv [" + (spnU.value as string) + "," + (spnV.value as string) + "]"        )        DestroyDialog rMatLibSetUVW    )    on BTNPICKFROMOBJECT picked obj do    (        local uv_mod = obj.modifiers[1]        if classOf uv_mod == Uvwmap do (                            spnU.value = uv_mod.utile            spnV.value = uv_mod.vtile        )    ))rollout rXmlMatSettings "Material Settings:" width:160 height:176(    GroupBox 'grp1' "Settings:" pos:[4,4] width:152 height:168 align:#left    spinner 'spn_visual_id' "" pos:[76,28] width:72 height:16 range:[0,10000,1] type:#integer scale:1 align:#left    label 'lbl1' "Visual ID:" pos:[12,28] width:60 height:16 align:#left    spinner 'spn_semantic_id' "" pos:[76,48] width:72 height:16 range:[0,10000,1] type:#integer scale:1 align:#left    label 'lbl2' "Semantic ID:" pos:[12,48] width:64 height:16 align:#left    spinner 'spn_u_tile' "" pos:[76,68] width:72 height:16 range:[0,10000,1] type:#integer scale:1 align:#left    label 'lbl3' "U Tile:" pos:[12,68] width:60 height:16 align:#left    spinner 'spn_v_tile' "" pos:[76,88] width:72 height:16 range:[0,10000,1] type:#integer scale:1 align:#left    label 'lbl5' "V Tile:" pos:[12,88] width:60 height:16 align:#left    button 'BTNPICKFROMOBJECT' "Pick From Object" pos:[8,112] width:140 height:24 align:#left    button 'BTNSAVE' "Save" pos:[8,140] width:140 height:24 align:#left         local ExtFunction = undefined        on BTNPICKFROMOBJECT pressed  do    (        local uv_mod = obj.modifiers[1]        if classOf uv_mod == Uvwmap do (                            spnU.value = uv_mod.utile            spnV.value = uv_mod.vtile        )    )    on BTNSAVE pressed do    (        local output_data = #(spn_visual_id.value, spn_semantic_id.value, spn_u_tile.value, spn_v_tile.value)        ExtFunction output_data          DestroyDialog rXmlMatSettings    ))CreateDialog rXmlMatSettingsrXmlMatSettings.ExtFunction = fn saveItemToXML data = (format "save XML:%\n" data)