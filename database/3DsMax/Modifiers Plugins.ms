--genClassID()plugin modifier  lockername:"Locker"classID:#(0xfb7ba38, 0x3809d735)version:1(  parameters main rollout:params  (    checkBox_01 type:#boolean ui:checkBox_01 default:false	seglen type:#float animatable:true ui:seglen default:20  )  rollout params "Locker Parameters"  (    checkbox checkBox_01 "Locked: " type:#boolean checked:false	spinner seglen "Seg Length: " range:[0.01,1e9,20]	button btn1 "Lock Camera" --pos:[4,32] width:95 height:30	button btn2 "Unlock Camera" --pos:[4,66] width:95 height:30	on seglen set val do messagebox "spi"	on checkBox_01 changed state do (		 if state then messagebox "lock" else messagebox "unlock"		  format "is locked?%" state		--if state then setTransformLockFlags selection #all else setTransformLockFlags selection #none	)	on btn1 pressed  do (		 messagebox "lock"		setTransformLockFlags $ #all	)	on btn2 pressed  do (		 messagebox "unlock"		setTransformLockFlags $ #none	)  ))/*--	lock active camera and its target(	activeCam = getActiveCamera()	setTransformLockFlags activeCam #all	setTransformLockFlags activeCam.target #all)--	unlock active camera and its target(	activeCam = getActiveCamera()	setTransformLockFlags activeCam #none	setTransformLockFlags activeCam.target #none)*//*b = box isselected:onm = EmptyModifier name:"Attribute"ca = attributes TestAttribute(parameters params rollout:params(float_param type:#float ui:ui_float_parambool_param type:#boolean ui:ui_bool_param)rollout params "Parameters"(slider ui_float_param "Float Parameter:" type:#float checkbox ui_bool_param "Boolean Parameter"))custattributes.add m caaddmodifier b m */