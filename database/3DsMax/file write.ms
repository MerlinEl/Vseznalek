--Write data as [string] "action:Move_To_X icon:28"fn writeDataToFile fname = (	local baction  = (getIniSetting Micra.TempProfileIni "Buttons" )	local ss = stringstream ""	format "\n" to:ss	for i=1 to baction.count do (				local btn_id  = baction[i]		local action = getINISetting Micra.TempProfileIni "Buttons"  btn_id		local icon = getINISetting Micra.TempProfileIni "Icons"  btn_id		format "% = action:% icon:%\n"  btn_id	action icon to:ss	)	print ss	close ss	flush ss	local user_dir = Micra.RootDir + "User\\"	local fpath = user_dir + fname	if  doesFileExist fpath do deleteFile fpath	local fstream = createFile fpath	format "%" (ss as string) to:fstream	close fstream	close ss)