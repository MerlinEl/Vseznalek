for i in $ where ((substring i.name 1 2) == "R ") do i.name = ("R" + (substring i.name 3 i.name.count)) --find word begins with R and space, replace with Rfor i in $ where (i.name[1] == "L") do i.name = ("l" + (substring i.name 1 i.name.count)) -- replace first two charsfor i in $ do i.name = (toLower i.name[1]) + (substring i.name 2 i.name.count) --replace first letter for lowerfor i in $ do print i.nameselect $mt_*for o in selection do (	for t=1 to o.tag_names.count do 	(		local tg = getNodeByName o.tag_names[t]		tg.name =replace tg.name 1 2 "tg"		o.tag_names[t] =replace o.tag_names[t] 1 2 "tg"	))for o in selection do o.name = replace o.name 1 2 "mt"