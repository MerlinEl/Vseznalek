    public void onPlayerJoin(PlayerJoinEvent evt) {        Player player = evt.getPlayer(); // The player who joined        PlayerInventory inventory = player.getInventory(); // The player's inventory        ItemStack itemstack = new ItemStack(Material.DIAMOND, 64); // A stack of diamonds             if (inventory.contains(itemstack)) {            inventory.addItem(itemstack); // Adds a stack of diamonds to the player's inventory            player.sendMessage("Welcome! You seem to be reeeally rich, so we gave you some more diamonds!");        }    }