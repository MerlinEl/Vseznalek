        // update an existing texture with new Bitmap data, or delete an existing texture        if (this.game.textures.exists('logo')) this.game.textures.remove('logo');        var img = document.createElement("img");        img.src = this.game.canvas.toDataURL("./img/phaser2.png");        img.addEventListener('load', () => {            this.game.textures.addImage('logo', img);        });