          toggleDebug: Boolean = false;      physics: {                //default: 'arcade',                arcade: {                    debug: true                    //gravity: { y: 0 }                }            }    create() {        this.physics.world.drawDebug = false;        this.toggleDebug = this.input.keyboard.addKey(Phaser.Input.Keyboard.KeyCodes.P);}    update() {        if (Phaser.Input.Keyboard.JustDown(this.game.toggleDebug)) {            if (this.physics.world.drawDebug) {                this.physics.world.drawDebug = false;                this.physics.world.debugGraphic.clear();            }            else {                this.physics.world.drawDebug = true;            }        }    }