cnt1 = 0cnt2 = 0pattern = #()for i=0 to (pow 10 2) do (	if cnt1 > 9 do cnt1 = 0	if cnt2 > 9 do (				cnt1+=1		cnt2 = 0	)	format "% %\n" cnt1 cnt2	append pattern (cnt1 as string + " " + cnt2 as string)	cnt2+=1)tran = #()f = openFile "c:\\temp\md.txt" mode:"r+"while not eof f do(   l = readline f   append tran l)close fprint tranfn isInteger str = (	if str == "."  or str == "<" do return false	execute str != undefined)fn replaceSpacesByPattern str = (		for p in pattern do (				local begin  = findString str p			if begin == undefined do continue		return replace str begin 3 ","		)	str)for i=1 to tran.count where isInteger tran[i][1] do (	tran[i] = replaceSpacesByPattern tran[i])for t in tran do print t