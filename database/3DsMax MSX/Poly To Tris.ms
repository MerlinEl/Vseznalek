Maxscript fn getPolyTable obj =(	local index = 0	format "obj:%\n" obj.name	for poly = 1 to polyop.getNumFaces obj do	(    		local numTris = (polyop.getFaceDeg obj poly) - 2		format "\tpoly:% tris:%\n" poly numTris		for face = index + 1 to index + numTris do (			format "\t\tface:%\n" face		 )		index += numTris	))getPolyTable $C#            int index = 0;            for ( int poly = 0; poly < _IMNMesh.Numf; poly++ ) {                IMNFace ngon = _IMNMesh.F(poly);                Max.Log("\tpoly:{0} verts:{1}", poly + 1, ngon.Vtx.Count);                for ( int i = 0; i < ngon.Vtx.Count; i++ ) Max.Log("\t\tvert:{0}", ngon.Vtx[i] + 1); //+1 Max indexing                int numTris = ngon.Deg - 2;                Max.Log("\tpoly:{0} tris:{1}", poly+1, numTris);                for ( int face = index; face < index + numTris; face++ ) {                    Max.Log("\t\tface:{0}", face+1);                 }                index += numTris;            }