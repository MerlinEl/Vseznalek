gray_mat = StandardMaterial diffusecolor:graytnXref_1 = xrefs.getXRefFile 1tnXref_2 = xrefs.getXRefFile 2tnXref_1.autoUpdate = false tnXref_2.autoUpdate = false for o in tnXref_1.tree.children do (	o.material = gray_mat	o.wirecolor = gray)for o in tnXref_2.tree.children do (	o.material = gray_mat	o.wirecolor = gray)for i = 1 to tnXref_1.tree.children.count do ( tnXref_1.tree.children[i].primaryVisibility = off tnXref_1.tree.children[i].inheritVisibility = on)for i = 1 to tnXref_2.tree.children.count do ( tnXref_2.tree.children[i].primaryVisibility = off tnXref_2.tree.children[i].inheritVisibility = on)