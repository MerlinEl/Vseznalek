struct formHolderDef (	form,	size = [200,270],	pos = [900,150],	title = "The Form",	fn open = (if iskindof form dotnetobject do form.Showmodeless()),	fn close = (if iskindof form dotnetobject do form.Close()),	fn onShown s a = 	(		print (s.WindowState.toString())	),	fn onMove s a = 	(		print (s.Location.toString())		print ::formHolder.size		print ::formHolder.pos	),	fn init =  	(		form = dotNetObject "MaxCustomControls.MaxForm"		form.size = dotNetObject "System.Drawing.Size" size.x size.y		form.Location = dotNetObject "System.Drawing.Point" pos.x pos.y		form.StartPosition = form.StartPosition.Manual		form.text = title			dotnet.addEventHandler form "Shown" onShown		dotnet.addEventHandler form "Move" onMove	),	initialized = init())::formHolder = formHolderDef()::formHolder.open()