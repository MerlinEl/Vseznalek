node_coll = $ --must have pivot at centergeometry_nodes = $ as array --must have pivot at centerfn createCollisionsNodes geo_nodes coll_node = (	for o in geo_nodes do (		local node_name = (trimright o.name "geometry")+"coll"		format "create collision node:%\n" node_name		local new_coll_node = copy coll_node		new_coll_node.name = node_name		new_coll_node.OutputPath = o.OutputPath		new_coll_node.pos = o.pos		new_coll_node.pos.y += 5000		new_coll_node.wirecolor = (color 177 27 88)             new_coll_node.pivot = [0,0,0]             o.pivot = [0,0,0]	))createCollisionsNodes geometry_nodes node_coll$.wirecolor = (color 177 27 88)for o in selection do print o.name