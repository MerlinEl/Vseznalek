fn fixSkeletonNames = (	bone_names = getIniSetting (Micra.TempDir+"BIP_PATTERN.ini") "BIP_PATTERN"	for n in bone_names do (		local new_name = ""		local success = TRUE		local b = getNodeByName n		if b == undefined do success = false		if success do (			new_name = getIniSetting (Micra.TempDir+"BIP_PATTERN.ini") "BIP_PATTERN" b.name			if new_name == undefined do (				success = false			)		)		format "replace [ % ] with [ % ] success:%\n" b.name new_name success		if success do b.name = new_name	))--fix Bip001 L LidTopfn resetEyeTRansform = (	local eli = (getNodeByName "Bip001 L LidTop")	eli.parent = undefined	deletekeys eli.controller #AllKeys		eli.pos.controller = linear_position ()	eli.rotation.controller = linear_rotation ()	eli.scale.controller = linear_scale ()	--deleteKey eli.pos.controller 0	--deleteKey eli.rotation.controller 0	--deleteKey eli.scale.controller 0	eli.transform = (		matrix3 \		[-1.09819,-0.0080479,-0.0625982] \		[-0.00804784,1.09997,-0.000229359] \		[-0.0625982,-0.000228614,1.09822] \ 		[-2.48888e-06,3.85264,100.992]	)	resetTransform eli	eli.parent = (getNodeByName "Bip001 Head"))fixSkeletonNames()resetEyeTRansform()