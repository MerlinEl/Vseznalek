fn inspectLS3DMultiMaterial mat lvl:0 = (	mcStructMan.import "mcLS3D" "mcLS3D" force:false	for i=1 to mat.count do(		local m = mat[i]		if not (isProperty m "name") do continue		local df = mcLS3D.getDuiffuseMap m		if lvl == 4 do format "\tdiffuse:%\n" df		if df != undefined then (							try (				local b = df.bitmap					if b != undefined then (					if lvl == 1 do format "(%) In Material [ % } Found Bitmap: [ % ]\n" i m.name b 				) else (					if lvl == 5 do format "(%) In Material [ % } Not Found Bitmap: [ % ]\n" i m.name b										local diffuse_path = (mcQuickMaterialsDialog.getTexturesFullPath m)[1]					if diffuse_path != undefined do (						format "\t\tdiffuse_path:% found:%\n" diffuse_path (doesFileExist diffuse_path)						local ls3d_mat = mcLS3D.getLs3DMaterial m						ls3d_mat.diffusemap.filename = diffuse_path					)				)			)			catch (								if lvl == 2 do format "(%) Error to read diffuse map in Material:%" i m.name			)			continue		) else (		     if lvl == 3 do format "(%)  Missing DiffuseMap in mat:% class:%\n" i m.name (classOf m)				local ls3d_mat = mcLS3D.getLs3DMaterial m				ls3d_mat.diffusemap = bitmaptexture()        )	))inspectLS3DMultiMaterial $.material lvl:1inspectLS3DMultiMaterial $.material lvl:2inspectLS3DMultiMaterial $.material lvl:5inspectLS3DMultiMaterial $.material lvl:3lh_road_industrial_d_v1  road_dlh_road_industrial_d_v1_side   road_d_side(450) Error to read diffuse map in Material:DX_(road_d)	diffuse:Map #22596:Bitmap(451) Error to read diffuse map in Material:DX_(road_d_side)	diffuse:undefined(1)  Missing DiffuseMap in mat:uni_color_grey.xml class:LS3DMaterial(1) In Material [ uni_color_grey.xml } Not Found Bitmap: [ undefined ](4) In Material [ DX_(grass_mud_gravel) } Not Found Bitmap: [ undefined ](7) In Material [ DX_(01 - Default) } Not Found Bitmap: [ undefined ](8) In Material [ DX_(DX_(grass_mud_darkgravel)) } Not Found Bitmap: [ undefined ](9) In Material [ DX_(new_ar