--copy/paste  camera animationstruct camTM (frame, cam, target)cam_anims = #()fn copyCameraAnimations frames = (	local anims = for f in frames do(		sliderTime = f		append cam_anims (camTm currentTime $.transform $.target.transform)	))fn pasteCameraAnimations = (	for data in cam_anims do (				sliderTime = data.frame			$.transform = data.cam		$.target.transform = data.target	))--with selected camera run this fncopyCameraAnimations #(0, 200, 300, 500, 100)--with selected camera and enabled Auto Key run this fnpasteCameraAnimations()for i = animationRange.start to animationRange.end do print i--if camera is brokencam_anims = #(camTm currentTime $[1].transform $[2].transform)append cam_anims (camTm currentTime $[1].transform $[2].transform)